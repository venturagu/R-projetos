library(tidyverse)
wines  <- read.csv("wine.csv", header = FALSE, sep = ",")
wines

#1 - Dimensões do data set
nrow(wines) #178 instancias
ncol(wines) #14 atributos

#2 - Remover instacias vazias
wines[complete.cases(wines),]
nrow(wines)

#Médias dos atributos 
mean(wines$V2) # Média de teor alcoólico %13.00062
mean(wines$V3) # Média de Ácido málico 2.336348
mean(wines$V4) # Média de cinzas 2.366517
mean(wines$V5) # Média de Alcalinidade de cinzas 19.49494
mean(wines$V6) # Média de Magnesio totais 99.74157
mean(wines$V7) # Média de Fenóis totais 2.295112
mean(wines$V8) # Média de Flavonóides
mean(wines$V9) # Média de Fenóis não flavanoides 0.3618539
mean(wines$V10) # Média de Proantocianidinas  1.590899
mean(wines$V11) # Média de intensidade de cor 5.05809
mean(wines$V12) # Média de Matiz 
mean(wines$V13) # Média de OD280 / OD315 de vinhos diluídos 2.611685
mean(wines$V14) # Media de Proline 746.8933

#Histogramas
par(mfrow=c(2,2))
alcool <- wines$V2
hist(alcool) # Alcool distribuidos nos valores 11% até 15%

acido_malico <- wines$V3
hist(acido_malico) # Discrepancia entre 1,75 contendo a maioria dos vinhos

cinzas <- wines$V4
hist(cinzas) # Maior quantidas de vinho com cinzas em 2,4

Alcalinidade <- wines$V5
hist(Alcalinidade)# Maior qunatidade entre 17 e 22

par(mfrow=c(2,2))
Magnesio <- wines$V6
hist(Magnesio) # Nivel de magnesio da maioria dos vinhos esta entre 10  e 100

phenol <- wines$V7
hist(phenol) # Nivel de phenol dos vinhos se concentra nas faixas 1.5 e 2.0 e 2.5 e 3.0

flav <- wines$V8
hist(flav) # Valor de flavonoides distribuidos entre 0.5 e 3.5

not_flav <- wines$V9
hist(not_flav) # Grande quantidade de vinhos no valor 0.25

par(mfrow=c(2,2))
Proantocianidinas <- wines$V10
hist(Proantocianidinas) # Proantocianidinas com niveis entre 1 e 2 para a maioria dos exemplares

intensidade <- wines$V11
hist(intensidade) # Valor de intensidade de cor entre 2 e 6 na maioria dos exemplares

tonalidade <- wines$V12
hist(tonalidade) # Tonalidade dos exemplares entre 0.6 e 1.2

diluicao <- wines$V13
hist(diluicao) # Valor de diluição entre 2.5 e 3.5 entre a maioria dos exemplares

proteina <- wines$V14
hist(proteina) # Valor de Proline dos exemplares a sua maioria em 500 e 600

#Box-plot
par(mfrow=c(2,2))
boxplot(alcool~wines$V1) # Mediana em %13, aparentemente sem outliers
boxplot(acido_malico~wines$V1) # Mediana em 2, com alguns valores outliers notados
boxplot(cinzas~wines$V1) # Outliers notados nas extremidades da faiza de valores
boxplot(Alcalinidade~wines$V1)

par(mfrow=c(2,2))
boxplot(Magnesio~wines$V1)
boxplot(phenol~wines$V1)
boxplot(flav~wines$V1)
boxplot(not_flav~wines$V1)

par(mfrow=c(2,2))
boxplot(Proantocianidinas~wines$V1)
boxplot(intensidade~wines$V1)
boxplot(tonalidade~wines$V1)
boxplot(diluicao~wines$V1)

boxplot(proteina~wines$V1)

